> library(rjson)
> library(RCurl)
> 
> getData <- function(num) {
+     base.url <- "http://localhost:8080/api/plot"
+     params <- paste0("?id=", num, "&begin=2001-01-01")
+     url <- paste0(base.url, params)
+ 
+     query <- getURL(url)
+     res <- fromJSON(query, unexpected.escape = "skip")$results
+ 
+     .getmu <- function(x){
+         print(x[1]$inner$mu)
+         return(x[1]$inner$mu)
+     }
+ 
+     mu <- lapply(res, .getmu)
+     return(mu)
+ }
> 
